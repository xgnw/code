[
  {
    "id": "7b5879332551bc6ea2d534f6e037f01c",
    "title": "减字木兰花",
    "authorName": "苏小小",
    "authorId": "7b5879332551bc6ed18f655cb5b41fac",
    "dynasty": "南北朝",
    "content": [
      "别离情绪。万里关山如底数。遣妾伤悲。未心郎家知不知。",
      "自从君去。数尽残冬春又暮。音信全乖。等到花开不见来。"
    ]
  },
  {
    "id": "7b587933b0fb23522481d7c4647d11b5",
    "title": "行路难",
    "authorName": "释宝月",
    "authorId": "7b587933b0fb2352d1457b72d1457b72",
    "dynasty": "南北朝",
    "content": [
      "君不见孤雁关外发，酸嘶度扬越。",
      "空城客子心肠断，幽闺思妇气欲绝。",
      "凝霜夜下拂罗衣，浮云中断开明月。",
      "夜夜遥遥徒相思，年年望望情不歇。",
      "寄我匣中青铜镜，情人为君除白发。",
      "行路难，行路难，",
      "夜闻南城汉使度，使我流泪忆长安。"
    ]
  },
  {
    "id": "7b587933b1c2a296e2fe338acd8cd507",
    "title": "子夜四时歌·渊冰厚三尺",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "渊冰厚三尺，素雪覆千里。",
      "我心如松柏，君情复何似？"
    ]
  },
  {
    "id": "7b587933b1c2a2967d84f686a18093ce",
    "title": "子夜四时歌·春林花多媚",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "春林花多媚，春鸟意多哀。",
      "春风复多情，吹我罗裳开。"
    ]
  },
  {
    "id": "7b587933b1c2a296b7558f67efa5bdb3",
    "title": "子夜歌·夜长不得眠",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "夜长不得眠，明月何灼灼。",
      "想闻散唤声，虚应空中诺。"
    ]
  },
  {
    "id": "7b587933b1c2a296cb7c6e50924ad719",
    "title": "子夜四时歌·春风动春心",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "春风动春心，流目瞩山林。",
      "山林多奇采，阳鸟吐清音。"
    ]
  },
  {
    "id": "7b587933b1c2a2965b6c65a93bc6c89e",
    "title": "子夜四时歌·自从别欢后",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "自从别欢后，叹音不绝响。",
      "黄檗向春生，苦心随日长。"
    ]
  },
  {
    "id": "7b587933b1c2a296aa41cb88cb80b94c",
    "title": "子夜四时歌·秋风入窗里",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "秋风入窗里，罗帐起飘扬。",
      "仰头看明月，寄情千里光。"
    ]
  },
  {
    "id": "7b587933b1c2a2963f65f292fc16278b",
    "title": "子夜歌·今夕已欢别",
    "authorName": "无名氏",
    "authorId": "7b587933b1c2a296d41d8cd9d41d8cd9",
    "dynasty": "南北朝",
    "content": [
      "今夕已欢别，合会在何时？",
      "明灯照空局，悠然未有期！"
    ]
  }
]