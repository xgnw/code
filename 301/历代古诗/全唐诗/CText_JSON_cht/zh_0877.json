[
  {
    "volume": 877,
    "sequence": 1,
    "title": "中宗引諺",
    "content": [
      "冬至長於歲。"
    ]
  },
  {
    "volume": 877,
    "sequence": 2,
    "title": "賈言忠引諺",
    "content": [
      "軍無媒，中道回。"
    ]
  },
  {
    "volume": 877,
    "sequence": 3,
    "title": "李績引諺別張文瓘",
    "content": [
      "千里相送，終於一別。"
    ]
  },
  {
    "volume": 877,
    "sequence": 4,
    "title": "路勵行引諺",
    "content": [
      "一人在朝，百人緩帶。"
    ]
  },
  {
    "volume": 877,
    "sequence": 5,
    "title": "郝南容引諺",
    "content": [
      "三公後，出死狗。"
    ]
  },
  {
    "volume": 877,
    "sequence": 6,
    "title": "員莊諺",
    "content": [
      "上有天堂，下有員莊。"
    ]
  },
  {
    "volume": 877,
    "sequence": 7,
    "title": "婁師德引諺",
    "content": [
      "卒客無卒主人。"
    ]
  },
  {
    "volume": 877,
    "sequence": 8,
    "title": "台中諺",
    "content": [
      "免巡未推，只得自知。"
    ]
  },
  {
    "volume": 877,
    "sequence": 9,
    "title": "宋守敬引諺",
    "content": [
      "雙陸無休勢。"
    ]
  },
  {
    "volume": 877,
    "sequence": 10,
    "title": "雒穀諺",
    "content": [
      "謥洞入黃泉。"
    ]
  },
  {
    "volume": 877,
    "sequence": 11,
    "title": "三門諺",
    "content": [
      "古無門匠墓。"
    ]
  },
  {
    "volume": 877,
    "sequence": 12,
    "title": "張果引諺",
    "content": [
      "娶婦得公主，平地生公府。"
    ]
  },
  {
    "volume": 877,
    "sequence": 13,
    "title": "哥舒翰引諺",
    "content": [
      "狐向窟嗥不祥。"
    ]
  },
  {
    "volume": 877,
    "sequence": 14,
    "title": "代宗引諺",
    "content": [
      "不癡不聾，不作阿家阿翁。"
    ]
  },
  {
    "volume": 877,
    "sequence": 15,
    "title": "鬼門關諺",
    "content": [
      "鬼門關，十人去，九不還。"
    ]
  },
  {
    "volume": 877,
    "sequence": 16,
    "title": "河北諺",
    "content": [
      "山東一條葛，無事莫撩撥。"
    ]
  },
  {
    "volume": 877,
    "sequence": 17,
    "title": "李振引諺",
    "content": [
      "百歲奴事三歲主。"
    ]
  },
  {
    "volume": 877,
    "sequence": 18,
    "title": "王彥章引諺",
    "content": [
      "人死留名，豹死留皮。"
    ]
  },
  {
    "volume": 877,
    "sequence": 19,
    "title": "孫光憲北夢瑣言引諺",
    "content": [
      "小舅小叔，相追相逐。"
    ]
  },
  {
    "volume": 877,
    "sequence": 20,
    "title": "諺",
    "content": [
      "棗子塞鼻孔，懸棲閣卻種。",
      "蟬鳴蛁蟟喚，黍種糕糜斷。"
    ]
  },
  {
    "volume": 877,
    "sequence": 21,
    "title": "寧茵事諺",
    "content": [
      "鵓鳩樹上鳴，意在麻子地。"
    ]
  },
  {
    "volume": 877,
    "sequence": 22,
    "title": "鸕鷀諺",
    "content": [
      "鸕鷀不打腳下塘。"
    ]
  },
  {
    "volume": 877,
    "sequence": 23,
    "title": "鹽鐵諺",
    "content": [
      "揚一益二。"
    ]
  },
  {
    "volume": 877,
    "sequence": 24,
    "title": "馮翊諺",
    "content": [
      "苦泉羊，洛水漿。"
    ]
  },
  {
    "volume": 877,
    "sequence": 25,
    "title": "丹徒諺",
    "content": [
      "生東吳，死丹徒。"
    ]
  },
  {
    "volume": 877,
    "sequence": 26,
    "title": "湖州裏諺",
    "content": [
      "放爾生，放爾命，放爾湖州做百姓。"
    ]
  },
  {
    "volume": 877,
    "sequence": 27,
    "title": "益陽諺",
    "content": [
      "長沙益陽，一時相b3。"
    ]
  },
  {
    "volume": 877,
    "sequence": 28,
    "title": "昭潭諺",
    "content": [
      "昭潭無底橘州浮。"
    ]
  },
  {
    "volume": 877,
    "sequence": 29,
    "title": "江右四郡諺",
    "content": [
      "筠袁贛吉，腦後插筆。"
    ]
  },
  {
    "volume": 877,
    "sequence": 30,
    "title": "徐聞諺",
    "content": [
      "欲拔貧，詣徐聞。"
    ]
  },
  {
    "volume": 877,
    "sequence": 31,
    "title": "隴西諺",
    "content": [
      "郎驅女驅，十馬九駒。安陽大角，十牛九犢。"
    ]
  },
  {
    "volume": 877,
    "sequence": 32,
    "title": "荊棺峽諺",
    "content": [
      "九子不葬父，一女打荊棺。"
    ]
  },
  {
    "volume": 877,
    "sequence": 33,
    "title": "南中諺",
    "content": [
      "秋收稻，夏收頭。"
    ]
  },
  {
    "volume": 877,
    "sequence": 34,
    "title": "事狐神諺",
    "content": [
      "無狐魅，不成村。"
    ]
  },
  {
    "volume": 877,
    "sequence": 35,
    "title": "李哲家怪引諺",
    "content": [
      "一雞死，一雞鳴。"
    ]
  },
  {
    "volume": 877,
    "sequence": 36,
    "title": "俗諺",
    "content": [
      "白日無談人，談人則害生。昏夜無說鬼，說鬼則怪至。"
    ]
  },
  {
    "volume": 877,
    "sequence": 37,
    "title": "李白名許雲封謎",
    "content": [
      "樹下彼何人，不語真吾好。語若及日中，煙霏謝成寶。"
    ]
  },
  {
    "volume": 877,
    "sequence": 38,
    "title": "許氏碑陰謎",
    "content": [
      "談馬礪畢，王田數七。"
    ]
  },
  {
    "volume": 877,
    "sequence": 39,
    "title": "大明寺壁語",
    "content": [
      "一人堂堂，二曜同光。泉深尺一，點去冰傍。二人相連，不欠一邊。三梁四柱列火然，除卻雙勾兩日全。"
    ]
  },
  {
    "volume": 877,
    "sequence": 40,
    "title": "曹著與客謎",
    "content": [
      "一物坐也坐，臥也坐，行也坐。",
      "一物坐也臥，立也臥，行也臥，走也臥，臥也臥"
    ]
  },
  {
    "volume": 877,
    "sequence": 41,
    "title": "客題青龍寺門",
    "content": [
      "龕龍去東海，時日隱西斜。敬文今不在，碎石入流沙。"
    ]
  },
  {
    "volume": 877,
    "sequence": 42,
    "title": "陶穀題南唐官舍壁",
    "content": [
      "西川狗，百姓眼。馬包兒，禦廚飯。"
    ]
  },
  {
    "volume": 877,
    "sequence": 43,
    "title": "謝小娥夢盜姓名",
    "content": [
      "車中猴，門東草。禾中走，一日夫。"
    ]
  }
]